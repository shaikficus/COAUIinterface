<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel"
                version="11.1.1.65.57" id="COAFindPGPageDef"
                Package="fuseuiapp.pageDefs">
  <parameters/>
  <executables>
    <variableIterator id="variables"/>
    <iterator Binds="COASegmentDefinitionsVO1" RangeSize="25"
              DataControl="COAAMDataControl"
              id="COASegmentDefinitionsVO1Iterator"/>
    <iterator Binds="COAMappingRulesVO1" RangeSize="25"
              DataControl="COAAMDataControl" id="COAMappingRulesVO1Iterator"/>
  </executables>
  <bindings>
    <methodAction id="findCOAValues" RequiresUpdateModel="true"
                  Action="invokeMethod" MethodName="findCOAValues"
                  IsViewObjectMethod="false" DataControl="COAAMDataControl"
                  InstanceName="COAAMDataControl.dataProvider">
      <NamedData NDName="sourceSystem"
                 NDValue="#{pageFlowScope.COAFindBean.findSourceSystemString}"
                 NDType="java.lang.String"/>
      <NamedData NDName="targetSystem"
                 NDValue="#{pageFlowScope.COAFindBean.findTargetSystemString}"
                 NDType="java.lang.String"/>
      <NamedData NDName="mappingRule"
                 NDValue="#{pageFlowScope.COAFindBean.findMappingRuleString}"
                 NDType="java.lang.String"/>
      <NamedData NDName="sourceSegment"
                 NDValue="#{pageFlowScope.COAFindBean.findSourceSegmentString}"
                 NDType="java.lang.String"/>
      <NamedData NDName="targetSegemnt"
                 NDValue="#{pageFlowScope.COAFindBean.findTargetSegmentString}"
                 NDType="java.lang.String"/>
      <NamedData NDName="sourceValue"
                 NDValue="#{pageFlowScope.COAFindBean.findSourceSegmentValueString}"
                 NDType="java.lang.String"/>
      <NamedData NDName="targetValue"
                 NDValue="#{pageFlowScope.COAFindBean.findTargetSegmentValueString}"
                 NDType="java.lang.String"/>
            <events xmlns="http://xmlns.oracle.com/adfm/contextualEvent">
        <event name="findCOAValuesEvent"/>
      </events>
    </methodAction>
    <list IterBinding="COASegmentDefinitionsVO1Iterator" StaticList="false"
          Uses="LOV_SystemName" id="SystemName" DTSupportsMRU="true"
          SelectItemValueMode="ListObject"/>
    <list IterBinding="COAMappingRulesVO1Iterator" StaticList="false"
          Uses="LOV_TargetSystem" id="TargetSystem" DTSupportsMRU="true"
          SelectItemValueMode="ListObject"/>
  </bindings>
</pageDefinition>
